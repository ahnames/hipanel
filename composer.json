{
    "name": "ahnames/hipanel",
    "type": "yii2-extension",
    "description": "AHnames implementation of HiPanel",
    "keywords": [
        "ahnames",
        "hipanel",
        "control panel",
        "hosting",
        "domains"
    ],
    "homepage": "https://ahnames.com/",
    "license": "proprietary",
    "support": {
        "email": "support@ahnames.com",
        "source": "https://github.com/ahnames/hipanel",
        "issues": "https://github.com/ahnames/hipanel/issues",
        "wiki": "https://github.com/ahnames/hipanel/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        }
    ],
    "require": {
        "hiqdev/hipanel-core": "*@dev",
        "hiqdev/hipanel-module-dashboard": "*@dev",
        "hiqdev/hipanel-module-client": "*@dev",
        "hiqdev/hipanel-module-domain": "*@dev",
        "hiqdev/hipanel-module-dns": "*@dev",
        "hiqdev/hipanel-module-ticket": "*@dev",
        "hiqdev/hipanel-module-server": "*@dev",
        "hiqdev/hipanel-module-finance": "*@dev",
        "hiqdev/hipanel-module-hosting": "*@dev",
        "hiqdev/hipanel-module-stock": "*@dev",
        "hiqdev/hipanel-module-document": "*@dev",
        "hiqdev/hipanel-module-mailing": "*@dev",
        "hiqdev/hipanel-mrdp": "<2.0 || dev-master",
        "hiqdev/hisite-module-news": "*@dev",
        "hiqdev/omnipay-ecoin": "*@dev",
        "hiqdev/omnipay-epayments": "*@dev",
        "hiqdev/omnipay-epayservice": "*@dev",
        "hiqdev/omnipay-interkassa": "*@dev",
        "hiqdev/omnipay-robokassa": "*@dev",
        "hiqdev/omnipay-okpay": "*@dev",
        "hiqdev/omnipay-qiwi": "*@dev",
        "hiqdev/omnipay-paxum": "*@dev",
        "hiqdev/omnipay-paypal": "*@dev",
        "hiqdev/omnipay-yandexmoney": "*@dev",
        "dercoder/omnipay-webmoney": "*@dev",
        "hiqdev/yii2-theme-adminlte": "*@dev"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "ahnames/hidev-ahnames": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ahnames\\hipanel\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "hisite": "src/config/hisite.php"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
